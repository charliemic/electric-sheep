<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">
    
    <!-- Electric Sheep Logo - Circular Abstract Design -->
    <!-- Same conceptual approach as launcher icon, but perfect circle -->
    <!-- Conceptual, abstract design - calm, pastoral, consciousness layers -->
    
    <!-- Main circle (perfect circle, centered) -->
    <!-- Represents: Pastoral calm, outer consciousness -->
    <path
        android:fillColor="#4A7C7E"
        android:pathData="M60,20
            A40,40 0 1,1 60,100
            A40,40 0 1,1 60,20 Z"/>
    
    <!-- Inner consciousness layer (smaller circle) -->
    <!-- Represents: Core consciousness, present moment -->
    <!-- Creates depth and calm -->
    <path
        android:fillColor="#4A7C7E"
        android:pathData="M60,50
            A10,10 0 1,1 60,70
            A10,10 0 1,1 60,50 Z"/>
    
    <!-- Subtle energy accent (abstract, not literal) -->
    <!-- Warm amber - matches launcher icon's accent color -->
    <!-- Represents: Subtle consciousness flow, not aggressive "electric" -->
    <!-- Circular segment accent -->
    <path
        android:fillColor="#D4885A"
        android:fillAlpha="0.5"
        android:pathData="M60,20
            A20,20 0 0,1 75,35
            A5,5 0 0,1 70,40
            A15,15 0 0,1 60,30
            Z"/>
    
</vector>
