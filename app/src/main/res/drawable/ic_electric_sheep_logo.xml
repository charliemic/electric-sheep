<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="120dp"
    android:height="120dp"
    android:viewportWidth="120"
    android:viewportHeight="120">
    
    <!-- Electric Sheep Logo - Circular Abstract Design with Concentric Circles -->
    <!-- Same conceptual approach as launcher icon, but perfect circle -->
    <!-- Conceptual, abstract design - calm, pastoral, consciousness layers -->
    
    <!-- Main circle (perfect circle, centered at 60,60) -->
    <!-- Represents: Pastoral calm, outer consciousness -->
    <!-- Slightly more blue: #4A7C8E -->
    <path
        android:fillColor="#4A7C8E"
        android:pathData="M60,20
            A40,40 0 1,1 60,100
            A40,40 0 1,1 60,20 Z"/>
    
    <!-- Concentric circles in center - perfectly centered at (60,60) -->
    <!-- Outer concentric circle (warm amber - more contrasting) -->
    <path
        android:fillColor="#D4885A"
        android:fillAlpha="0.7"
        android:pathData="M60,50
            A15,15 0 1,1 60,80
            A15,15 0 1,1 60,50 Z"/>
    
    <!-- Middle concentric circle (lighter blue-teal - more blue hint) -->
    <!-- #6A9CAE - more blue -->
    <path
        android:fillColor="#6A9CAE"
        android:fillAlpha="0.9"
        android:pathData="M60,55
            A10,10 0 1,1 60,75
            A10,10 0 1,1 60,55 Z"/>
    
    <!-- Inner concentric circle (solid blue-teal, core consciousness) -->
    <!-- #4A7C8E - more blue, perfectly centered -->
    <path
        android:fillColor="#4A7C8E"
        android:pathData="M60,58
            A7,7 0 1,1 60,72
            A7,7 0 1,1 60,58 Z"/>
    
</vector>
